<Window x:Class="AlphaZulu.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AlphaZulu" Height="386" Width="921" Icon="/NATO_flag.ico">
    <!-- Title="AlphaZulu" Height="386" Width="921" Icon="pack://siteoforigin:,,,/NATO_flag.ico">-->
    <Grid>
        <TextBox x:Name="InputTBox" Height="23" Margin="10,33,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
        <TextBox Margin="10,82,10,10" TextWrapping="Wrap" Text="{Binding Output, UpdateSourceTrigger=PropertyChanged}"/>
        <TextBlock Margin="10,12,0,0" TextWrapping="Wrap" Text="Input" VerticalAlignment="Top" HorizontalAlignment="Left" Width="28"/>
        <TextBlock Margin="10,61,0,0" TextWrapping="Wrap" Text="Output" Height="16" VerticalAlignment="Top" HorizontalAlignment="Left" Width="38"/>

    </Grid>
</Window>
